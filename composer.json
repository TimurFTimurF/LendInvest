{
    "name": "aim/lendinvest",
    "type": "project",
    "description": "Test project",
    "require": {
        "phpunit/phpunit": "^8.3"
    },

    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php"
    },
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
